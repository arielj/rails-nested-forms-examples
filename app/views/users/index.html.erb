<h1>Users</h1>

<%= link_to 'New user', new_user_path, class: 'btn btn-primary' %>
<%= link_to 'New user with address', new_with_address_users_path, class: 'btn btn-primary' %>
<%= link_to 'New user with pets', new_with_pets_users_path, class: 'btn btn-primary' %>
<%= link_to 'New user with variable pets', new_with_variable_pets_users_path, class: 'btn btn-primary' %>
<%= link_to 'New user with address and pets', new_with_address_and_pets_users_path, class: 'btn btn-primary' %>
<br />
<br />

<h3>List</h3>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.to_label, user %>
    </li>
  <% end %>
</ul>